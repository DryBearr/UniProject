@using DTOs

@model IEnumerable<ResponseUserDto>
    
@if (Model != null && Model.Any())
{
    <table>
        <tr>
            <th>UserId</th>
            <th>UserName</th>
        </tr>
        @foreach(var user in Model)
        {
            <tr>
                <td>@user.UserId</td>
                <td>@user.Username</td>
            </tr>
        }
    </table>
}
else
{
    <p>No user found.</p>
}